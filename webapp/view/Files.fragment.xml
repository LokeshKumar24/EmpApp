<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
<Dialog  title="Upload Files" id="filesUpload" >
<sf:SimpleForm editable="true">
				<Label text=""></Label>
				<Input id="InputIdFS" placeholder="Enter Emp Id"></Input>
				<Label text=""></Label>
				<Input id="InputNameFS" type="Text" placeholder="Enter Emp Name "></Input>
                 <Label text=""></Label>
                  <Label text="Enter Date" labelFor="DPFS"/>
		<DatePicker
			id="DPFS"
			placeholder="Enter Date"
			change="handleChange"
			class="sapUiSmallMarginBottom"/>
                <Label text=""></Label>
                <Label text="File Uploader"></Label>
				<u:FileUploader id="fileUploaderFS"/>
				<Button text="Upload" type="Accept" press="onUploadFile"/>
				<Label></Label>
				
				
				
				<Label></Label>
				<Button text="Submit" type="Emphasized" press="onUploadTS"></Button>
                <Button text="close" icon="sap-icon://decline" press="onClose" type="Reject"></Button>
			</sf:SimpleForm>
            </Dialog>
	
</core:FragmentDefinition>